             x:Class="PersonagensApp.Views.ProfileUserPage"
             Title="Cadastro">
  <ContentPage.Content>
    <ScrollView>
      <StackLayout>
        <Image x:Name="imgCover" />
        <Image x:Name="imgProfile" />
        <Label x:Name="lblNome" />
        <Label x:Name="lblIdade" />
        <Label x:Name="lblGenero" />
        <Label x:Name="lblEmail" />
      </StackLayout>
    </ScrollView>
  </ContentPage.Content>

</ContentPage>